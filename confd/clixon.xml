<clixon-config xmlns="http://clicon.org/config">
  <CLICON_CONFIGFILE>./clixon.xml</CLICON_CONFIGFILE>
  <CLICON_CONFIGDIR>/usr/local/etc/clixon.d</CLICON_CONFIGDIR>
  <CLICON_FEATURE>*:*</CLICON_FEATURE>
  <CLICON_YANG_DIR>/usr/local/share/clixon</CLICON_YANG_DIR>
  <CLICON_YANG_MODULE_MAIN>clixon-config</CLICON_YANG_MODULE_MAIN>
  <CLICON_CLI_MODE>hello</CLICON_CLI_MODE>
  <CLICON_CLISPEC_DIR>/usr/local/lib/hello/clispec</CLICON_CLISPEC_DIR>
  <CLICON_SOCK>./hello.sock</CLICON_SOCK>
  <CLICON_BACKEND_PIDFILE>./hello.pidfile</CLICON_BACKEND_PIDFILE>
  <CLICON_XMLDB_DIR>./hello</CLICON_XMLDB_DIR>
  <CLICON_STARTUP_MODE>init</CLICON_STARTUP_MODE>
  <restconf>
     <enable>true</enable>
  </restconf>
</clixon-config>

